<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.bitacademy.java41.dao.TeamStudentDao">
	
	<select id="teamstudentList"  resultType="TeamStudent" parameterType="int">
	    select TSNO,TNO, TSNAME , TSBACKNO , TSPHOTO, TSSTATUS
	    from TORY_TSTUDENT
	    where TNO=#{TNO}
	</select>
	
</mapper> 






